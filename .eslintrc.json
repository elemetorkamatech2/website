{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "max-len": ["error", { "code": 120 }],
        "linebreak-style": ["error", "unix"],
        "object-curly-newline": ["error", { "multiline": true, "consistent": true }]
    }
}
